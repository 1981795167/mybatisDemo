<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.chenyee.quotecenter.model.ChenyeeQcPurchase"
		table="chenyee_qc_purchase">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>
		
		<property
			name="fdManufacturePrice"
			column="fd_manufacture_price"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdStandbyPrice"
			column="fd_standby_price"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdFinancialPrice"
			column="fd_financial_price"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdLogisticsPrice"
			column="fd_logistics_price"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdQualcommPatentPrice"
			column="fd_qualcomm_patent_price"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdSoftwarePatentPrice"
			column="fd_software_patent_price"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdHolster"
			column="fd_holster"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdMembrane"
			column="fd_membrane"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdUpdateTime"
			column="fd_update_time"
			update="true"
			insert="true"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />
			
		<!-- chenyee liuxf 2018.3.20 modify start -->
		<many-to-one
			name="fdProduct"
			column="fd_product_id"
			insert="true"
			update="true"
			not-null="false" />
		<!-- chenyee liuxf 2018.3.20 modify end -->


	</class>
</hibernate-mapping>
