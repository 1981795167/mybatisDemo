<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.chenyee.quotecenter.model.ChenyeeQcHardware"
		table="chenyee_qc_hardware">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdHardwarePrice"
			column="fd_hardware_price"
			update="true"
			insert="true"
			length="36"
			not-null="true" />

		<property
			name="fdUpdateTime"
			column="fd_update_time"
			update="true"
			insert="true"
			not-null="true" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="true" />			
		
		<!-- chenyee liuxf 2018.3.20 modify start -->
		<many-to-one
			name="fdProduct"
			column="fd_product_id"
			insert="true"
			update="true"
			not-null="false" />
		<!-- chenyee liuxf 2018.3.20 modify end -->


	</class>
</hibernate-mapping>
