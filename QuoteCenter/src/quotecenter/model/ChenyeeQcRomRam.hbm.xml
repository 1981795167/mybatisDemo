<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.chenyee.quotecenter.model.ChenyeeQcRomRam"
		table="chenyee_qc_rom_ram">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdRomRam"
			column="fd_rom_ram"
			update="true"
			insert="true"
			length="36"
			not-null="true" />

		<property
			name="fdBomPrice"
			column="fd_bom_price"
			update="true"
			insert="true"
			length="36"
			not-null="true" />

		<property
			name="fdPurchasePrice"
			column="fd_purchase_price"
			update="true"
			insert="true"
			length="36"
			not-null="true" />

		<property
			name="fdProductPrice"
			column="fd_product_price"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdPresetPercent"
			column="fd_preset_percent"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdPresetBottomPrice"
			column="fd_preset_bottom_price"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		 <!-- chenyee liuxf 2018.3.16 modify start -->
		<many-to-one
			name="fdProduct"
			column="fd_product_id"
			insert="true"
			update="true"
			not-null="false" />
		<!-- chenyee liuxf 2018.3.16 modify end -->

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
