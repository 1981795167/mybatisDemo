<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.chenyee.quotecenter.model.ChenyeeQcCustOrder"
		table="chenyee_qc_cust_order">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdProjectName"
			column="fd_project_name"
			update="true"
			insert="true"
			length="200"
			not-null="true" />

		<property
			name="fdCustomerName"
			column="fd_customer_name"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdRomRam"
			column="fd_rom_ram"
			update="true"
			insert="true"
			length="36"
			not-null="true" />

		<property
			name="fdProductPrice"
			column="fd_product_price"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdBottomPrice"
			column="fd_bottom_price"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdPercent"
			column="fd_percent"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdUpdateTime"
			column="fd_update_time"
			update="true"
			insert="true"
			not-null="true" />

		<!-- chenyee liuxf 2018.3.16 modify start -->
		<many-to-one
			name="fdProduct"
			column="fd_product_id"
			insert="true"
			update="true"
			not-null="false" />
		<!-- chenyee liuxf 2018.3.16 modify end -->

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />


	</class>
</hibernate-mapping>
