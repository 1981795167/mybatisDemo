<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.chenyee.quotecenter.model.ChenyeeQcBom"
		table="chenyee_qc_bom">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdBomName"
			column="fd_bom_name"
			update="true"
			insert="true"
			length="200"
			not-null="true" />

		<property
			name="fdBomPrice"
			column="fd_bom_price"
			update="true"
			insert="true"
			length="36"
			not-null="true" />

		<property
			name="fdBomCode"
			column="fd_bom_code"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdFactory"
			column="fd_factory"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdApplication"
			column="fd_application"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdVersionDescription"
			column="fd_version_description"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdVersion"
			column="fd_version"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdMaterielCode"
			column="fd_materiel_code"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdDimension"
			column="fd_dimension"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdMaterielPrice"
			column="fd_materiel_price"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdSerialNumber"
			column="fd_serial_number"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdMaterialDescription"
			column="fd_material_description"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdBomPriceUpdateTime"
			column="fd_bom_price_update_time"
			update="true"
			insert="true"
			not-null="false" />
			
		<!-- chenyee liuxf 2018.3.20 modify start -->
		<many-to-one
			name="fdProduct"
			column="fd_product_id"
			insert="true"
			update="true"
			not-null="false" />
		<!-- chenyee liuxf 2018.3.20 modify end -->


	</class>
</hibernate-mapping>
