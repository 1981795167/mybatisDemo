<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class
		name="com.landray.kmss.chenyee.quotecenter.model.ChenyeeQcProduct"
		table="chenyee_qc_product">
		<id
			name="fdId"
			column="fd_id"
			length="36">
			<generator
				class="assigned" />
		</id>

		<property
			name="fdProjectName"
			column="fd_project_name"
			update="true"
			insert="true"
			length="200"
			not-null="true" />

		<property
			name="fdStandard"
			column="fd_standard"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdAndroidVersion"
			column="fd_android_version"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdRearCamera"
			column="fd_rear_camera"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdBatteryCapacity"
			column="fd_battery_capacity"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdFrontCamera"
			column="fd_front_camera"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdScreeSize"
			column="fd_scree_size"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="fdHardwarePlatform"
			column="fd_hardware_platform"
			update="true"
			insert="true"
			length="36"
			not-null="false" />

		<property
			name="docCreateTime"
			column="doc_create_time"
			update="true"
			insert="true"
			not-null="false" />

		<property
			name="fdUpdateTime"
			column="fd_update_time"
			update="true"
			insert="true"
			not-null="false" />

		<many-to-one
			name="docCreator"
			column="doc_creator_id"
			insert="true"
			update="true"
			not-null="false" />

        <!-- liuxf 2018.3.21 modify start -->
		
		<list
			name="fdBom"
			cascade="all-delete-orphan"
			lazy="true">
			<key
				column="fd_product_id" />
			<index column="position"></index>
			<one-to-many
				class="com.landray.kmss.chenyee.quotecenter.model.ChenyeeQcBom" />
		 </list>
		
		 <list
			name="fdPurchase"
			cascade="all-delete-orphan"
			lazy="true">
			<key
				column="fd_product_id" />
			<index column="position"></index>
			<one-to-many
				class="com.landray.kmss.chenyee.quotecenter.model.ChenyeeQcPurchase" />
		 </list>		
		
		 <list
			name="fdHardware"
			cascade="all-delete-orphan"
			lazy="true">
			<key
				column="fd_product_id" />
			<index column="position"></index>
			<one-to-many
				class="com.landray.kmss.chenyee.quotecenter.model.ChenyeeQcHardware" />
		 </list> 
		
		<list
			name="fdEarphone"
			cascade="all-delete-orphan"
			lazy="true">
			<key
				column="fd_product_id" />
			<index column="position"></index>
			<one-to-many
				class="com.landray.kmss.chenyee.quotecenter.model.ChenyeeQcEarphone" />
		 </list>

        <!-- liuxf 2018.3.21 modify end -->
        
		<!-- liuxf 2018.3.16 modify start -->
		 <list
			name="fdRomrams"
			cascade="all-delete-orphan"
			lazy="true">
			<key
				column="fd_product_id" />
			<index column="position"></index>
			<one-to-many
				class="com.landray.kmss.chenyee.quotecenter.model.ChenyeeQcRomRam" />
		 </list>
		 
 		
		<list
			name="fdCustomerOrders"
			cascade="all-delete-orphan"
			lazy="true">
			<key
				column="fd_product_id" />
			<index column="position"></index>
			<one-to-many
				class="com.landray.kmss.chenyee.quotecenter.model.ChenyeeQcCustOrder" />
		 </list>
		 <!-- liuxf 2018.3.16 modify end -->


	</class>
</hibernate-mapping>
